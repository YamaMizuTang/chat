{
  "entryFile": "./src/app.ts",
  "noImplicitAdditionalProperties": "throw-on-extras",
  "controllerPathGlobs": [
    "./src/controllers/*.ts"
  ],
  "spec": {
    "securityDefinitions": {
      "jwt": {
        "type": "http",
        "scheme": "bearer"
      }
    },
    "outputDirectory": "./",
    "specVersion": 3
  },
  "routes": {
    "routesDir": "./src",
    "authenticationModule": "./src/libs/authentication.ts"
  }
}
